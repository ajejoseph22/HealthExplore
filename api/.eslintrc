{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-var": ["off"],
    "one-var": ["off"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "import/prefer-default-export": "off",
    "brace-style": "off"
  }
}
